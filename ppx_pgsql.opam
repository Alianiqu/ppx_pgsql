]
tags: ["postgresql" "pgocaml" "ppx"]
available: [
    ocaml-version >= "4.03"
]
dev-repo: "https://github.com/tizoc/ppx_pgsql.git"
bug-reports: "https://github.com/tizoc/ppx_pgsql/issues"
