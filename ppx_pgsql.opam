opam-version: "1.2"
name: "ppx_pgsql"
version: "0.1"
author: "Bruno Deferrari <utizoc@gmail.com>"
maintainer: "Bruno Deferrari <utizoc@gmail.com>"
homepage: "https://github.com/tizoc/ppx_pgsql"
license: "BSD-3"
build: [
  ["jbuilder" "build" "-p" name "-j" jobs]
]
depends: [
    "pgocaml"
    "ppx_driver"
